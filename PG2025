                local args = {}
                
                for i, orb in ipairs(game:GetService("Workspace").__THINGS.Orbs:GetChildren()) do
                    table.insert(args, tonumber(orb.Name))
                    orb.Transparency = 1
                end
                Network["Orbs: Collect"]:FireServer(args) -- Collect Orbs (Coins | Bags | Drops)
            end
        end)

    end
end

